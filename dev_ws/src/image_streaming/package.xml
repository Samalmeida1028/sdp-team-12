<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>image_streaming</name>
  <version>0.0.1</version>
  <description>This is the image publishing node for our arUco detection library</description>
  <maintainer email="salmeida@umass.edu">sdp12</maintainer>
